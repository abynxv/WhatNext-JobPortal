{
    "builds": [
        {
            "src": "host/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb",
                "runtime": "python3.9"
            }

        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "host/wsgi.py"
        }
    ]
}